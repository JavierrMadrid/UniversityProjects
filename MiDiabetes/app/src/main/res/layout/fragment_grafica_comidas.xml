<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ap="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fragment_graficas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.javier.midiabetes.GraficaComidasFragment">

    <Spinner
        android:id="@+id/spinnerGraficaHidratos"
        android:layout_width="312dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView47"
        ap:layout_constraintLeft_toLeftOf="parent"
        ap:layout_constraintRight_toRightOf="parent"
        ap:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="5dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="UnknownIdInLayout" />

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/pieChart3"
        android:layout_width="343dp"
        android:layout_height="480dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/spinnerGraficaHidratos"
        android:layout_marginRight="0dp"
        ap:layout_constraintRight_toRightOf="parent"
        ap:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        ap:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        ap:layout_constraintTop_toBottomOf="@+id/spinnerGraficaHidratos"
        ap:layout_constraintHorizontal_bias="0.529"
        ap:layout_constraintVertical_bias="0.0" />


</RelativeLayout>
