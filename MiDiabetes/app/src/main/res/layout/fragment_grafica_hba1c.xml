<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ap="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fragment_graficas"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.javier.midiabetes.GraficaHbA1cFragment">

    <TextView
        android:id="@+id/textView47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/pieChart2"
        android:layout_alignLeft="@+id/pieChart2"
        android:layout_alignRight="@+id/pieChart2"
        android:layout_alignStart="@+id/pieChart2"
        android:text="Historial de los valores introducidos de la hemoglobina glicosilada:"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/ap_charcoal"
        android:textSize="15sp"
        android:typeface="sans"
        ap:layout_constraintHorizontal_bias="0.625"
        ap:layout_constraintLeft_toLeftOf="parent"
        ap:layout_constraintRight_toRightOf="parent"
        ap:layout_constraintTop_toTopOf="parent" />

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/pieChart2"
        android:layout_width="336dp"
        android:layout_height="490dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textView47"
        android:layout_marginTop="10dp"
        ap:layout_constraintBottom_toBottomOf="parent"
        ap:layout_constraintHorizontal_bias="0.555"
        ap:layout_constraintLeft_toLeftOf="parent"
        ap:layout_constraintRight_toRightOf="parent"
        ap:layout_constraintTop_toBottomOf="@+id/textView47"
        ap:layout_constraintVertical_bias="0.0" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/btnAÃ±adirHemoglobina"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:clickable="true"
        ap:backgroundTint="@color/colorAccent"
        ap:fabSize="mini"
        ap:srcCompat="@drawable/anadir"
        tools:layout_editor_absoluteY="491dp"
        tools:layout_editor_absoluteX="280dp" />


</RelativeLayout>
